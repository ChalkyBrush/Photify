<% @pictures.each do |picture| %>
<div class="picturecard">
<br>
  <li class="title"><%= picture[:title] %></li>
  <li class ="comment_content">Added by: <%= picture.user.first_name %> <%= picture.user.last_name %></li>
  <%= link_to image_tag(picture.url, :class => "index"), "/pictures/#{picture.id}"%>
  
 </div>
 
<% end %>